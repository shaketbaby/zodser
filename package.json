{
  "name": "zodser",
  "version": "0.1.2",
  "description": "A simple zod serialiser",
  "main": "./dest/index.js",
  "types": "./dest/index.d.ts",
  "scripts": {
    "to-release": "npm run release [<newversion> | major | minor | patch]",
    "release": "npm run build && npm version",
    "build": "tsc src/*.ts --target es2018 --moduleResolution node --module commonjs --esModuleInterop --declaration --outDir dest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:shaketbaby/zodser.git"
  },
  "homepage": "https://github.com/shaketbaby/zodser",
  "bugs": {
    "url": "https://github.com/shaketbaby/zodser/issues"
  },
  "keywords": [
    "zod",
    "serialise",
    "serialisation"
  ],
  "author": "Shaketbaby",
  "license": "MIT",
  "volta": {
    "node": "18.14.0"
  },
  "peerDependencies": {
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "ts-node": "^10.9.1"
  }
}
